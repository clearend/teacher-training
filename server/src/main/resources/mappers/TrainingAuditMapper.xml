<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.training.core.mapper.TrainingAuditMapper">
 <select id="examineTraining" >
     update training_audit set audit_status=#{auditStatus},audit_time=now(),audit_remark=#{auditRemark}
                             ,is_del=#{isDel},update_time=now() where training_audit_id=#{trainingAuditId}

 </select>
</mapper>
